SELECT* from plant_store_database order by selling_price;
SELECT sum(selling_price) from plant_store_database;
SELECT avg(selling_price) FROM plant_store_database;
SELECT min(selling_price) FROM plant_store_database;
SELECT max(selling_price) FROM plant_store_database;
SELECT * FROM plant_store_database WHERE selling_price>150;
SELECT * FROM plant_store_database WHERE cost<=80 ORDER BY cost ASC;
